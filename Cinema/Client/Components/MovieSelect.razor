@using Cinema.Shared.DTO

<MudSelect @bind-Value="@Movies" Variant="Variant.Outlined" Label="Movies">
   @foreach(var movie in Movies) {
        <MudSelectItem Value="@(movie.Name)">@(movie.Name)</MudSelectItem>
   }
</MudSelect>

@code {
    [Parameter]
    public List<MovieDTO> Movies { get; set; } = new List<MovieDTO>();
}